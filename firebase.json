{
  "hosting": {
    "public": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "cleanUrls":true,
    "trailingSlash":true
  },
  "rewrites": [
    {
      "source": "**",
      "destination": "/$1.html",
      "regex": "^(.*)$",
      "type":"Rewrite"
    }
  ]
}
